    <PostBuildEvent>set /p KSP_DIR=&lt;"$(ProjectDir)LocalDev\ksp_dir.txt"

set /p MONO_EXE_DIR=&lt;"$(ProjectDir)LocalDev\mono_exe.txt"

set /p PDB2MDB_EXE_DIR=&lt;"$(ProjectDir)LocalDev\pdb2mdb_exe.txt"

set /p ZA_DIR=&lt;"$(ProjectDir)LocalDev\7za_dir.txt"

xcopy "$(TargetPath)" "$(ProjectDir)Distribution\GameData\$(ProjectName)\Plugins\" /Y

xcopy "$(TargetDir)$(ProjectName).pdb" "$(ProjectDir)Distribution\GameData\$(ProjectName)\Plugins\" /Y

cd "$(TargetDir)"
"%25MONO_EXE_DIR%25" "%25PDB2MDB_EXE_DIR%25" "$(TargetFileName)"
xcopy "$(TargetDir)$(ProjectName).dll.mdb" "$(ProjectDir)Distribution\GameData\$(ProjectName)\Plugins\" /Y

where /q %25ZA_DIR%25:7za.exe
if %25ERRORLEVEL%25 == 1 (
   echo Cannot find 7-zip to package build
) else (
   echo Packaging build
  ) 
if exist "$(SolutionDir)$(ProjectName)_V@(VersionNumber).zip" del "$(SolutionDir)$(ProjectName)_V@(VersionNumber).zip"
   
%25ZA_DIR%257za.exe a -tzip -r  "$(SolutionDir)$(ProjectName)_V@(VersionNumber).zip" "$(ProjectDir)Distribution\GameData"
cd "$(ProjectDir)Distribution\GameData\$(ProjectName)\Icons"
dir /b &gt; "$(SolutionDir)IconsExclude.txt"
c:\7za\7za d -r  -x@"$(SolutionDir)IconsExclude.txt" "$(SolutionDir)$(ProjectName)_V@(VersionNumber).zip" "*.ddsified" "*.pdb" "*.mdb" "*.png"
xcopy /E /Y "$(ProjectDir)Distribution\GameData" "%25KSP_DIR%25\GameData"
</PostBuildEvent>
